        android:theme="@style/Theme.AgeInMinutes2">
        <activity
            android:name="com.aneesh.ageinminutes.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>